    .const $0 16 0x0
    .const $1 16 0x1
    .const $2 16 0x2
    .const $3 16 0x3
    .const $4 16 0x4
    SEND $0, [proc_0_0], $0;
    SEND $1, [proc_0_0], $1;
    SEND $2, [proc_0_0], $2;
    SEND $3, [proc_0_0], $3;
    // SET $0, 0x0;
    SET $1, 0x01;
    SET $2, 0x02;
    SET $3, 0x03;
